			<portlet-mode>config</portlet-mode>			
		</supports>
		<supported-locale>en</supported-locale>
		<portlet-info>
			<title>Spring Display</title>
			<short-title>Spring Display</short-title>
			<keywords>Spring Display</keywords>
		</portlet-info>
	</portlet>
